{
  "Log": {
    "Level": "info"
  },
  "Cores": [
    {
      "Type": "sing",
      "OriginalPath": "/opt/V2bX-Nodemix/configs/example/sing_origin.json"
    }
  ],
  "Nodes": [
    {
      "Core": "sing",
      "ApiHost": "https://your-panel.com",
      "ApiKey": "your-api-key",
      "NodeID": 1,
      "NodeType": "shadowsocks",
      "Timeout": 30,
      "ListenIP": "::",
      "SendIP": "0.0.0.0",
      "DeviceOnlineMinTraffic": 1000,
      "TCPFastOpen": true,
      "SniffEnabled": true,
      "EnableDNS": true,
      "CertConfig": {
        "CertMode": "none",
        "RejectUnknownSni": false,
        "CertDomain": "example.com",
        "CertFile": "/etc/V2bX/fullchain.cer",
        "KeyFile": "/etc/V2bX/cert.key",
        "Email": "v2bx@github.com",
        "Provider": "cloudflare",
        "DNSEnv": {
          "EnvName": "env1"
        }
      },
      "LimitConfig": {
        "EnableDeviceLimit": true,
        "DeviceLimit": 0
      }
    }
  ]
}
